// 4-) Faça um algoritmo que receba 20 numeros, coloque num vetor, e depois imprima os números em ordem decrescente

var vetor = [10,7,8,4,5,1,3,2,6,9,14,99,88,75,36,48,91,28,154,20];

para(var i = 0; i < 10; i++){

    para(var j = 1; j < 10; j++ ){

        se(vetor[j] > vetor[j-1]){

            var aux = vetor[j-1];
            vetor[j-1] = vetor[j];
            vetor[j] = aux;
        }
    }
}

para(var i = 0; i<10; i++){
    escreva(vetor[i]);
}